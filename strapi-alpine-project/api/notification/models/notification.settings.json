{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "notifications",
  "info": {
    "name": "Notification"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "eventDescription": {
      "type": "string"
    },
    "checked": {
      "type": "boolean"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user"
    }
  }
}
