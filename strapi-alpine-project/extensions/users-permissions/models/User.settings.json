    },
    "failure_records": {
      "via": "users",
      "collection": "failure-record"
    },
    "machines": {
      "collection": "machine",
      "via": "users"
    },
    "facilities": {
      "collection": "facility",
      "via": "labour"
    },
    "planned_maintenance_crons": {
      "via": "users",
      "collection": "planned-maintenance"
    },
    "level": {
      "type": "integer",
      "required": false,
      "max": 2,
      "min": 0,
      "default": 2
    },
    "company": {
      "model": "company",
      "via": "users"
    },
    "cost": {
      "type": "integer",
      "default": 12
